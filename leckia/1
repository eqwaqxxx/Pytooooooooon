print("–û–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ —Å–µ–∑–æ–Ω–∞")
season = int(input("–í–≤–µ–¥–∏—Ç–µ –º–µ—Å—è—Ü –æ—Ç 1-12 : "))

match season:
    case 1|2|12 :
        print("–ó–ò–º–∞ ‚ùÑÔ∏è‚õÑ")
    case 3 | 4 | 5:
        print("–í–µ—Å–Ω–∞ üçÉ")
    case 6 | 7 | 8:
        print("–ª–µ—Ç–æ ‚òÄÔ∏è")
    case 9 | 10 | 11:
        print("–û—Å–µ–Ω—å ü¶å")
    case _:
        print("–ù–µ–≤–µ—Ä–Ω–æ–µ —á–∏—Å–ª–æ")
