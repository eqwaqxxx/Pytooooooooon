found = False

for a in range(1, 151):
    if found: break
    for b in range(a, 151):
        if found: break
        for c in range(b, 151):
            if found: break
            for d in range(c, 151):
                sum_5 = a**5 + b**5 + c**5 + d**5
                e = int(sum_5 ** (1/5))
                if e**5 == sum_5 and e <= 150:
                    print(f'a={a}, b={b}, c={c}, d={d}, e={e}')
                    print(f'Сумма a+b+c+d+e = {a + b + c + d + e}')
                    found = True
                    break
                elif (e + 1)**5 == sum_5 and (e + 1) <= 150:
                    e += 1
                    print(f'a={a}, b={b}, c={c}, d={d}, e={e}')
                    print(f'Сумма a+b+c+d+e = {a + b + c + d + e}')
                    found = True
                    break
