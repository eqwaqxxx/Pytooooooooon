print("У вас есть 100 денег .")
for bikov in range (1,11):
    for korov in range (1,21):
         for telenok in range (1,201):
             if bikov *10 + korov*5 + telenok*0.5 == 100:
                print (f"{bikov} — количество быков;{korov} — количество коров;{telenok} — количество теленок.")
