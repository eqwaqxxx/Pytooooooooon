print("Рассматривается невисокосный год из 365 дней.")
for n in range (1,13):
    for k in range (1,12):
         for m in range (1,11):
             if (28 * n) + (30 * k) + (31 * m) == 365 and m + k + n == 12:
                 print (f"{n} — количество месяцев по 28 дней;{k} — количество месяцев по 30 дней;{m} — количество месяцев по 31 дню.")
